{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] [{Context}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AppSettings": {
    "BatchProcessing": {
      "DefaultMaxLoad": 10,
      "DefaultMaxDraw":  5
    }
  }
}